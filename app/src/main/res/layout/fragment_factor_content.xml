<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/border_bottom_left"
    android:descendantFocusability="beforeDescendants"
    android:focusableInTouchMode="true"
    android:orientation="vertical">

    <androidx.core.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <androidx.cardview.widget.CardView
                android:id="@+id/card_header"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="5dp"
                android:background="@android:color/white"
                app:cardCornerRadius="6dp"
                app:cardElevation="2dp"
                app:cardPreventCornerOverlap="false">


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:weightSum="5">


                    <LinearLayout
                        android:id="@+id/linearNumber"
                        android:layout_width="0px"
                        android:layout_height="match_parent"
                        android:layout_weight="1.5"
                        android:gravity="center"
                        android:orientation="vertical">

                        <EditText
                            android:id="@+id/edit_number"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:background="@null"
                            android:ems="10"
                            android:focusable="true"
                            android:focusableInTouchMode="true"
                            android:gravity="center"
                            android:inputType="numberSigned"
                            android:layoutDirection="rtl"
                            android:text="1001"
                            android:textDirection="rtl"
                            android:textSize="14sp" />

                        <TextView
                            android:id="@+id/edit_date"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="10dp"
                            android:background="@null"
                            android:gravity="center"
                            android:layoutDirection="rtl"
                            android:text="1399/05/05"
                            android:textDirection="rtl"
                            android:textSize="14sp" />
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/linearTitle"
                        android:layout_width="0px"
                        android:layout_height="match_parent"
                        android:layout_weight="2"
                        android:gravity="center"
                        android:orientation="vertical">

                        <EditText
                            android:id="@+id/edit_title"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:background="@null"
                            android:ems="10"
                            android:focusable="true"
                            android:focusableInTouchMode="true"
                            android:gravity="center"
                            android:inputType="textPersonName"
                            android:layoutDirection="rtl"
                            android:text="فاکتور فروش"
                            android:textDirection="rtl"
                            android:textSize="16sp"
                            android:textStyle="bold" />

                        <EditText
                            android:id="@+id/edit_subtitle"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="10dp"
                            android:background="@null"
                            android:ems="10"
                            android:focusable="true"
                            android:focusableInTouchMode="true"
                            android:gravity="center"
                            android:hint="نام فروشگاه ..."
                            android:inputType="textPersonName"
                            android:layoutDirection="rtl"
                            android:textDirection="rtl"
                            android:textSize="14sp" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="0px"
                        android:layout_height="wrap_content"
                        android:layout_weight="1.5"
                        android:gravity="right"
                        android:orientation="vertical">

                        <com.sajednt.html2pdf.function.SquareImageView
                            android:id="@+id/image_logo"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_margin="3dp"
                            android:scaleType="fitCenter"
                            android:src="@mipmap/ic_launcher" />
                    </LinearLayout>
                </LinearLayout>

            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:id="@+id/card_info"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="5dp"
                android:background="@android:color/white"
                app:cardCornerRadius="6dp"
                app:cardElevation="2dp"
                app:cardPreventCornerOverlap="false">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/codecolor"
                    android:gravity="right"
                    android:layoutDirection="rtl"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/textView6"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:layoutDirection="rtl"
                        android:rotation="90"
                        android:text="فروشنده"
                        android:textColor="@android:color/white"
                        android:textDirection="rtl"
                        android:textStyle="bold" />

                    <androidx.cardview.widget.CardView
                        android:id="@+id/card_info_seller"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_margin="2dp"
                        android:background="@android:color/white"
                        app:cardCornerRadius="6dp"
                        app:cardElevation="2dp"
                        app:cardPreventCornerOverlap="false">


                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="vertical">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layoutDirection="rtl"
                                android:orientation="horizontal"
                                android:padding="5dp"
                                android:textDirection="rtl"
                                android:weightSum="8">

                                <TextView
                                    android:id="@+id/textView111"
                                    android:layout_width="0px"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:text="نام :" />

                                <EditText
                                    android:id="@+id/edit_seller_name"
                                    android:layout_width="0px"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="3"
                                    android:background="@null"
                                    android:focusable="true"
                                    android:focusableInTouchMode="true"
                                    android:hint="نام .."
                                    android:inputType="textPersonName"
                                    android:layoutDirection="rtl"
                                    android:textDirection="rtl"
                                    android:textSize="14sp" />

                                <TextView
                                    android:id="@+id/textView33"
                                    android:layout_width="0px"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:text="تلفن :" />

                                <EditText
                                    android:id="@+id/edit_seller_phone"
                                    android:layout_width="0px"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="3"
                                    android:background="@null"
                                    android:focusable="true"
                                    android:focusableInTouchMode="true"
                                    android:hint="شماره تلفن .."
                                    android:inputType="textPersonName"
                                    android:layoutDirection="rtl"
                                    android:textDirection="rtl"
                                    android:textSize="14sp" />


                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layoutDirection="rtl"
                                android:orientation="horizontal"
                                android:padding="5dp"
                                android:textDirection="rtl"
                                android:weightSum="8">

                                <TextView
                                    android:id="@+id/textView156"
                                    android:layout_width="0px"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1.5"
                                    android:gravity="right"
                                    android:text="کد ملی :" />

                                <EditText
                                    android:id="@+id/edit_seller_code"
                                    android:layout_width="0px"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="2.5"
                                    android:background="@null"
                                    android:focusable="true"
                                    android:focusableInTouchMode="true"
                                    android:hint="کد ملی .."
                                    android:inputType="textPersonName"
                                    android:layoutDirection="rtl"
                                    android:textDirection="rtl"
                                    android:textSize="14sp" />

                                <TextView
                                    android:id="@+id/textView8"
                                    android:layout_width="0px"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1.5"
                                    android:text="کد پستی :" />

                                <EditText
                                    android:id="@+id/edit_seller_zip"
                                    android:layout_width="0px"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="2.5"
                                    android:background="@null"
                                    android:focusable="true"
                                    android:focusableInTouchMode="true"
                                    android:hint="کد پستی  .."
                                    android:inputType="textPersonName"
                                    android:layoutDirection="rtl"
                                    android:textDirection="rtl"
                                    android:textSize="14sp" />


                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layoutDirection="rtl"
                                android:orientation="horizontal"
                                android:padding="5dp"
                                android:textDirection="rtl">

                                <TextView
                                    android:id="@+id/textView106"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:gravity="right"
                                    android:text="آدرس :" />

                                <EditText
                                    android:id="@+id/edit_seller_address"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:background="@null"
                                    android:focusable="true"
                                    android:focusableInTouchMode="true"
                                    android:gravity="top|right"
                                    android:hint="آدرس .."
                                    android:inputType="textMultiLine"
                                    android:layoutDirection="rtl"
                                    android:maxLines="2"
                                    android:paddingRight="5dp"
                                    android:textDirection="rtl"
                                    android:textSize="14sp" />


                            </LinearLayout>
                        </LinearLayout>


                    </androidx.cardview.widget.CardView>


                </LinearLayout>

            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:id="@+id/card_info_buy"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="5dp"
                android:background="@android:color/white"
                app:cardCornerRadius="6dp"
                app:cardElevation="2dp"
                app:cardPreventCornerOverlap="false">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/codecolor"
                    android:gravity="right"
                    android:layoutDirection="rtl"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/textView1121"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:layoutDirection="rtl"
                        android:paddingLeft="3dp"
                        android:rotation="90"
                        android:text=" خریدار "
                        android:textColor="@android:color/white"
                        android:textDirection="rtl"
                        android:textStyle="bold" />

                    <androidx.cardview.widget.CardView
                        android:id="@+id/card_info_buyer"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_margin="2dp"
                        android:background="@android:color/white"
                        app:cardCornerRadius="6dp"
                        app:cardElevation="2dp"
                        app:cardPreventCornerOverlap="false">


                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="vertical">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layoutDirection="rtl"
                                android:orientation="horizontal"
                                android:padding="5dp"
                                android:textDirection="rtl"
                                android:weightSum="8">

                                <TextView
                                    android:id="@+id/textView11421"
                                    android:layout_width="0px"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:text="نام :" />

                                <EditText
                                    android:id="@+id/edit_buyer_name"
                                    android:layout_width="0px"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="3"
                                    android:background="@null"
                                    android:focusable="true"
                                    android:focusableInTouchMode="true"
                                    android:hint="نام .."
                                    android:inputType="textPersonName"
                                    android:layoutDirection="rtl"
                                    android:textDirection="rtl"
                                    android:textSize="14sp" />

                                <TextView
                                    android:id="@+id/textView3dw3"
                                    android:layout_width="0px"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:text="تلفن :" />

                                <EditText
                                    android:id="@+id/edit_buyer_phone"
                                    android:layout_width="0px"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="3"
                                    android:background="@null"
                                    android:focusable="true"
                                    android:focusableInTouchMode="true"
                                    android:hint="شماره تلفن .."
                                    android:inputType="textPersonName"
                                    android:layoutDirection="rtl"
                                    android:textDirection="rtl"
                                    android:textSize="14sp" />


                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layoutDirection="rtl"
                                android:orientation="horizontal"
                                android:padding="5dp"
                                android:textDirection="rtl"
                                android:weightSum="8">

                                <TextView
                                    android:id="@+id/textView1dw56"
                                    android:layout_width="0px"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1.5"
                                    android:gravity="right"
                                    android:text="کد ملی :" />

                                <EditText
                                    android:id="@+id/edit_buyer_code"
                                    android:layout_width="0px"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="2.5"
                                    android:background="@null"
                                    android:focusable="true"
                                    android:focusableInTouchMode="true"
                                    android:hint="کد ملی .."
                                    android:inputType="textPersonName"
                                    android:layoutDirection="rtl"
                                    android:textDirection="rtl"
                                    android:textSize="14sp" />

                                <TextView
                                    android:id="@+id/textViewqw8"
                                    android:layout_width="0px"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1.5"
                                    android:text="کد پستی :" />

                                <EditText
                                    android:id="@+id/edit_buyer_zip"
                                    android:layout_width="0px"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="2.5"
                                    android:background="@null"
                                    android:focusable="true"
                                    android:focusableInTouchMode="true"
                                    android:hint="کد پستی .."
                                    android:inputType="textPersonName"
                                    android:layoutDirection="rtl"
                                    android:textDirection="rtl"
                                    android:textSize="14sp" />


                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layoutDirection="rtl"
                                android:orientation="horizontal"
                                android:padding="5dp"
                                android:textDirection="rtl">

                                <TextView
                                    android:id="@+id/textView1we06"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:gravity="right"
                                    android:text="آدرس :" />

                                <EditText
                                    android:id="@+id/edit_buyer_address"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:background="@null"
                                    android:focusable="true"
                                    android:focusableInTouchMode="true"
                                    android:gravity="top|right"
                                    android:hint="آدرس"
                                    android:inputType="textMultiLine"
                                    android:layoutDirection="rtl"
                                    android:maxLines="2"
                                    android:paddingRight="5dp"
                                    android:textDirection="rtl"
                                    android:textSize="14sp" />


                            </LinearLayout>
                        </LinearLayout>


                    </androidx.cardview.widget.CardView>


                </LinearLayout>

            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:id="@+id/card_items"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="5dp"
                android:background="@android:color/white"
                app:cardBackgroundColor="@color/codecolor"
                app:cardCornerRadius="6dp"
                app:cardElevation="2dp"
                app:cardPreventCornerOverlap="false">


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">


                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="horizontal"
                        android:padding="5dp">

                        <Button
                            android:id="@+id/image_add"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="10dp"
                            android:layout_marginLeft="2dp"
                            android:padding="3dp"
                            android:layout_alignParentLeft="true"
                            android:background="?android:attr/selectableItemBackground"
                            android:drawableLeft="@drawable/ic_add"
                            android:text="مورد جدید "
                            android:textColor="@android:color/white"/>
                        <Button
                            android:id="@+id/btnItemList"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="10dp"
                            android:layout_marginLeft="2dp"
                            android:padding="3dp"
                            android:background="?android:attr/selectableItemBackground"
                            android:drawableLeft="@drawable/ic_item_list"
                            android:text="انتخاب از لیست "
                            android:layout_toRightOf="@id/image_add"
                            android:textColor="@android:color/white"/>

                        <TextView
                            android:id="@+id/textView1dw"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_marginRight="10dp"
                            android:gravity="center|right"
                            android:text="موارد فاکتور"
                            android:layout_alignParentRight="true"
                            android:textColor="@android:color/white"
                            android:textStyle="bold"
                            android:layout_centerVertical="true"/>

                    </RelativeLayout>

                    <androidx.cardview.widget.CardView
                        android:id="@+id/card_noitem"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_margin="2dp"
                        android:background="@android:color/white"
                        app:cardCornerRadius="6dp"
                        app:cardElevation="2dp"
                        app:cardPreventCornerOverlap="false">


                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:weightSum="5">

                            <TextView
                                android:id="@+id/text_noItem"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:padding="5dp"
                                android:text="آیتمی وجود ندارد" />
                        </LinearLayout>

                    </androidx.cardview.widget.CardView>

                    <androidx.recyclerview.widget.RecyclerView
                        android:id="@+id/recyclerItems"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:isScrollContainer="false" />

                </LinearLayout>

            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:id="@+id/card_price"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="5dp"
                android:background="@android:color/white"
                app:cardCornerRadius="6dp"
                app:cardElevation="2dp"
                app:cardPreventCornerOverlap="false">


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:padding="3dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="right"
                        android:orientation="horizontal"
                        android:weightSum="5">

                        <TextView
                            android:id="@+id/text_total"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="3"
                            android:background="@drawable/border_corener_left"
                            android:gravity="right"
                            android:padding="5dp"
                            android:text="0" />

                        <TextView
                            android:id="@+id/textViwd2"
                            android:layout_width="0px"
                            android:layout_height="wrap_content"
                            android:layout_weight="2"
                            android:background="@drawable/border_corener_right"
                            android:padding="5dp"
                            android:text="جمع فاکتور :" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="right"
                        android:orientation="horizontal"
                        android:weightSum="5">

                        <EditText
                            android:id="@+id/edit_takhfif"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="3"
                            android:background="@drawable/border_edittext"
                            android:gravity="right"
                            android:inputType="numberSigned"
                            android:padding="5dp"
                            android:text="0" />

                        <CheckBox
                            android:id="@+id/check_takhfif"
                            android:layout_width="0px"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:background="@drawable/border_edittext"
                            android:layoutDirection="rtl"
                            android:text="%"
                            android:textDirection="rtl" />

                        <TextView
                            android:id="@+id/textawdView2"
                            android:layout_width="0px"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:background="@drawable/border_edittext"
                            android:padding="5dp"
                            android:text="تخفیف :" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="right"
                        android:orientation="horizontal"
                        android:weightSum="5">

                        <EditText
                            android:id="@+id/edit_tax"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="3"
                            android:background="@drawable/border_edittext"
                            android:gravity="right"
                            android:inputType="numberSigned"
                            android:padding="5dp"
                            android:text="0" />

                        <CheckBox
                            android:id="@+id/check_tax"
                            android:layout_width="0px"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:background="@drawable/border_edittext"
                            android:checked="true"
                            android:layoutDirection="rtl"
                            android:text="%"
                            android:textDirection="rtl" />

                        <TextView
                            android:id="@+id/textView2"
                            android:layout_width="0px"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:background="@drawable/border_edittext"
                            android:padding="5dp"
                            android:text="مالیات :" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="right"
                        android:orientation="horizontal"
                        android:weightSum="5">

                        <LinearLayout
                            android:layout_width="0px"
                            android:layout_height="wrap_content"
                            android:layout_weight="3"
                            android:background="@drawable/border_bottom_left"
                            android:orientation="vertical">

                            <TextView
                                android:id="@+id/text_pay"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"

                                android:gravity="right"
                                android:padding="5dp"
                                android:text="0 ریال" />

                            <TextView
                                android:id="@+id/text_pay2"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"

                                android:gravity="right"
                                android:padding="5dp" />
                        </LinearLayout>

                        <TextView
                            android:id="@+id/textVidwdwd2"
                            android:layout_width="0px"
                            android:layout_height="match_parent"
                            android:layout_weight="2"
                            android:background="@drawable/border_bottom_right"
                            android:padding="5dp"
                            android:text="مبلغ قابل پرداخت :" />
                    </LinearLayout>
                </LinearLayout>

            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:id="@+id/card_extra"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="5dp"
                android:background="@android:color/white"
                app:cardCornerRadius="6dp"
                app:cardElevation="2dp"
                app:cardPreventCornerOverlap="false">


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="right"
                        android:orientation="vertical">

                        <EditText
                            android:id="@+id/edit_extra"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:ems="10"
                            android:gravity="start|top"
                            android:hint="توضیحات فاکتور ..."
                            android:inputType="textMultiLine"
                            android:layoutDirection="rtl"
                            android:lines="2"
                            android:textDirection="rtl" />
                    </LinearLayout>
                </LinearLayout>

            </androidx.cardview.widget.CardView>

        </LinearLayout>
    </androidx.core.widget.NestedScrollView>
</LinearLayout>
